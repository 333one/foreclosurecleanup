    <footer class="-rightLeftPaddingMediumStatic">
        
        <div class="-standardWidth">
            <div class="footer__container -topBottomPaddingLarge">

                <div>
                <% if (activeLink === 'index') { %> 
                    <p class="footerNavigation__activeUnit">
                <% } else { %>
                    <p class="footerNavigation__unit">
                <% } %>
                    <a href="/">HOME</a></p>

                <% if (activeLink === 'foreclosure-cleanup-vendor-list') { %> 
                    <p class="footerNavigation__activeUnit">
                <% } else { %>
                    <p class="footerNavigation__unit">
                <% } %>
                    <a href="/foreclosure-cleanup-vendor-list">VENDOR LIST</a></p>

                <% if (loggedIn != true) { %> 
                    <% if (activeLink === 'register') { %> 
                    <p class="footerNavigation__activeUnit">
                    <% } else { %>
                    <p class="footerNavigation__unit">
                    <% } %>
                    <a href="/register">REGISTER</a></p>
                <% } %>

                <% if (activeLink === 'contact') { %> 
                    <p class="footerNavigation__activeUnit">
                <% } else { %>
                    <p class="footerNavigation__unit">
                <% } %>
                    <a href="mailto:<%- contactEmail %>">CONTACT</a></p>

                </div>

                <div class="footer__logo"><a href="/"><img src="images/foreclosure-cleanup-logo-bw.svg" alt="Foreclosure Cleanup logo"></a></div>
            </div>

            <div class="footer__privacyTermsContainer -bottomMarginLarge">

                <a href="/privacy-policy" class="
                    <% if (activeLink === 'privacy-policy') { %>
